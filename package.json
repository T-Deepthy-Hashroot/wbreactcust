{
  "name": "workbench",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.3.1",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "chartist": "0.10.1",
    "crypto-js": "^3.1.9-1",
    "debug": "^3.2.6",
    "history": "4.7.2",
    "moment": "2.29.2",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "4.1.2",
    "perfect-scrollbar": "1.3.0",
    "query-string": "^6.8.2",
    "react": "^16.4.1",
    "react-big-calendar": "0.18.0",
    "react-bootstrap-sweetalert": "4.2.3",
    "react-chartist": "0.13.1",
    "react-datetime": "2.14.0",
    "react-dom": "^16.4.1",
    "react-google-maps": "9.4.5",
    "react-jvectormap": "0.0.2",
    "react-moment": "^0.7.0",
    "react-nouislider": "2.0.1",
    "react-number-format": "^3.3.4",
    "react-router-dom": "4.3.1",
    "react-scripts": "^3.1.1",
    "react-spinners": "^0.5.4",
    "react-swipeable-views": "0.12.13",
    "react-switch": "^5.0.0",
    "react-table": "^6.8.6",
    "react-tagsinput": "3.19.0",
    "save": "^2.3.2"
  },
  "scripts": {
    "war": "makewar %npm_package_name% %npm_package_version%",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build && npm run war",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run build"
  },
  "homepage": "/workbench/",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
